@page "/Profile"
<div class="mainView ">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h3 class="actorTitles">Your profile</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <img src="images/icons8-female-profile-96.png" class="rounded-circle" alt="testImages" style="width: 200px; padding-right:30px">
            </div>
        </div>
        <div class="row">

            <div class="col-md-6">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-6">
                            <input class="form-control" id="inputUsername" aria-describedby="uNameHelp" placeholder="Username">
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-6">
                            <input type="email" class="form-control" id="inputEmail" aria-describedby="emailHelp" placeholder="Email">
                        </div>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-6">
                            <input class="form-control" id="inputName" aria-describedby="nameHelp" placeholder="Name">
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="row">
            <div class="col-md-6">
                <div class="col-md-12 actorBio">
                    <button type="button" class="btn btn-primary">Save</button>
                </div>
            </div>
        </div>
    </div>

   
    
</div>

